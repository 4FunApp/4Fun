<?xml version="1.0" encoding="UTF-8"?>
 <validators>
	<field name="username">
		<field-validator type="requiredstring">
			<param name="trim">true</param>
			<message>账号不能为空</message>
		</field-validator>
	</field>
	<field name="password">
		<field-validator type="requiredstring">
			<param name="trim">true</param>
			<message>密码不能为空</message>
		</field-validator>
	</field>
</validators>